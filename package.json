    "build:sidecar-linux": "pyinstaller -c -F --clean --name main-x86_64-unknown-linux-gnu --distpath src-tauri/bin/api src/backends/main.py",
    "build:icons": "pnpm tauri icon public/app-icon.png",
    "build": "next build",
    "tauri": "tauri",
