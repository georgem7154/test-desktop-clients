  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "install-reqs": "pnpm install && pip3 install -r requirements.txt",
    "dev": "next dev",
    "build:sidecar-winos": "pyinstaller -c -F --clean --name main-x86_64-pc-windows-msvc --distpath src-tauri/bin/api src/backends/main.py",
